function gradeCalculator(score){
    if (score < 0 || score > 100 || isNaN(score) || typeof score !=='number' ) {
        return 'Invalid Score';
    } else if (score > 79){
        return 'A';
    } else if (score >= 60) {
        return 'B';
    } else if (score >= 49) {
        return 'C';
    } else if (score >= 40) {
        return 'D';
    } else {
        return 'E'
    }
}